{
  "name": "cpu-controller",
  "version": "1.0.0",
  "description": "Android CPU Control Client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-builder --win --x64"
  },
  "keywords": ["electron", "android", "cpu", "adb"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.0"
  },
  "dependencies": {
    "chart.js": "^4.4.8"
  },
  "build": {
    "appId": "com.example.cpucontroller",
    "productName": "CPU Controller",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "directories": {
      "output": "dist"
    }
  }
}